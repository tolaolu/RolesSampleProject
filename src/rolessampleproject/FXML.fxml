<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rolessampleproject.FXMLController">
   <children>
      <ComboBox fx:id="roleComboBox" layoutX="109.0" layoutY="108.0" onAction="#onComboChange" prefHeight="25.0" prefWidth="132.0" promptText="Select Your Role" />
      <Label fx:id="resultLabel" layoutX="43.0" layoutY="49.0" prefHeight="30.0" prefWidth="132.0" text="Displayor">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="addItemsBtn" layoutX="12.0" layoutY="108.0" mnemonicParsing="false" onAction="#onButtonPress" prefHeight="25.0" prefWidth="81.0" text="Add Items" />
      <ListView fx:id="listView" layoutX="248.0" layoutY="26.0" prefHeight="150.0" prefWidth="130.0" />
      <Button fx:id="selectBtn" layoutX="45.0" layoutY="151.0" mnemonicParsing="false" onAction="#onSelectBtnPress" prefHeight="25.0" prefWidth="142.0" text="Select" />
   </children>
</AnchorPane>
